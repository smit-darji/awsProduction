{
    "family": "dev-task-smit",
    "containerDefinitions": [
        {
            "image": "997817439961.dkr.ecr.ap-south-1.amazonaws.com/dev-repo-smit:latest",
            "name": "dev-container-smit",
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::997817439961:role/ecsTaskExecutionRole" 
}
 
