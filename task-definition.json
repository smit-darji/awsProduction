{
    "family": "Hello-task-smit",
    "containerDefinitions": [
        {
            "image": ":demo",
            "name": "hello-Container-smit",
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::997817439961:role/ecsTaskExecutionRole" 
}
